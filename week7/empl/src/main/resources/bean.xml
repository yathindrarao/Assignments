<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="empllist" class="week7.emplxml.employeelist" scope="singleton">
<property name="maps">
	<map>
		<entry key="1">
			<bean class="week7.emplxml.employee">
				<property name="id" value="1" />
				<property name="name" value="aditya" />
				<property name="city" value="ghaziabad" />
			</bean>
		</entry>
		<entry key="2">
			<bean class="week7.emplxml.employee">
				<property name="id" value="2" />
				<property name="name" value="ethan" />
				<property name="city" value="jalandhar" />
			</bean>
		</entry>
		<entry key="3">
			<bean class="week7.emplxml.employee">
				<property name="id" value="3" />
				<property name="name" value="Benji" />
				<property name="city" value="noida" />
			</bean>
		</entry>
	</map>
</property>
</bean>


</beans>
